{
  "name": "adobe-stock-uploader",
  "version": "1.0.0",
  "description": "Adobe Stock Image Uploader - Web Interface",
  "main": "server.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "ts-node -r tsconfig-paths/register server.ts",
    "build": "vite build",
    "start": "npm run build && ts-node server.ts",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "lint-staged": {
    "*.{ts,tsx}": [
      "vitest related --run"
    ],
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.13.11",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "lucide-react": "^0.553.0",
    "multer": "^2.0.0-rc.4",
    "openai": "^6.8.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^7.2.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tsconfig-paths": "^4.2.0",
    "vitest": "^4.0.8"
  }
}

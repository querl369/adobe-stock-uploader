{
  "name": "adobe-stock-uploader",
  "version": "1.0.0",
  "description": "Adobe Stock Image Uploader - Web Interface",
  "main": "server.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "start": "ts-node src/index.ts",
    "web": "npm run build:frontend && ts-node server.ts",
    "dev": "npm run build:frontend && ts-node server.ts",
    "build:frontend": "tsc public/app.ts --outDir public --target ES2020 --lib ES2020,DOM --skipLibCheck",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "lint-staged": {
    "*.{ts,tsx}": [
      "vitest related --run"
    ],
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.13.11",
    "cloudinary": "^2.8.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "multer": "^2.0.0-rc.4",
    "openai": "^6.8.1",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "vitest": "^4.0.8"
  }
}
